<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zkzong.sb.mybatis.mapper.UsersMapper">
    <select id="getAllUsers" resultType="com.zkzong.sb.mybatis.domain.Users">
        select id, user_name userName, age from t_users
    </select>

    <select id="findByUserName" resultType="com.zkzong.sb.mybatis.domain.Users">
        select id, user_name userName, age from t_users where name = #{name}
    </select>

    <insert id="insertOne">
        insert into t_users(user_name, age) values(#{userName}, #{age})
    </insert>
</mapper>