<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zkzong.mp.mapper.test.TempMapper">

    <select id="getById" resultType="com.zkzong.mp.entity.Temp">
        select id, user_name userName
        from temp
        where id = #{id}
