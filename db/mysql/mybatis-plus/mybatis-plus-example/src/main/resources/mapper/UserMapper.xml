<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.mp.org.example.UserMapper">

    <select id="getById" resultType="org.example.mp.entity.TUser">
        select id, user_name userName, age, email
        from t_user
        where id = #{id}
    </select>

</mapper>